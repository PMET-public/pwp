  "version": "0.2.0",
  "configurations": [
    {
      "name": "pwp",
      "type": "pwa-node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/pwp.js"
    },
    {
      "name": "pwp new-project",
      "type": "pwa-node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/pwp.js",
      "args": [
        "new-project"
      ]
    },
    {
      "name": "pwp list",
      "type": "pwa-node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/pwp.js",
      "args": [
        "list"
      ]
    }
  ]
}